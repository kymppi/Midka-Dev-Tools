{
  "name": "midka-dev-tools",
  "version": "1.0.8",
  "description": "Dev Tools by Midka",
  "main": "index.js",
  "bin": {
    "@midka/midka-dev-tools": "bin/midka-dev-tools",
    "midka-dev-tools": "bin/midka-dev-tools"
  },
  "dependencies": {
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.2.1",
    "arg": "^5.0.0",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "esm": "^3.2.25",
    "execa": "^5.0.0",
    "figlet": "^1.5.0",
    "listr": "^0.14.3",
    "ncp": "^2.0.0",
    "pkg-install": "^1.0.0",
    "prompts": "^2.4.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/listr": "^0.14.2",
    "@types/ncp": "^2.0.4",
    "@types/node": "^15.0.1",
    "@types/prompts": "^2.0.9"
  },
  "scripts": {
    "start": "npm run build && npm run global",
    "lint": "tslint -p . --fix",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g && cgx",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish"
  },
  "keywords": [
    "cli",
    "dev-tools"
  ],
  "author": "Midka",
  "license": "MIT",
  "files": [
    "bin/",
    "src/",
    "templates/"
  ]
}
